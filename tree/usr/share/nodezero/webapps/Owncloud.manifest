AppName="Owncloud"
AppUrl="none"
AppType="archive"
AppDescription="Edit, store, share and synchronize files, contacts and calendars"
AppWwwDir="owncloud"
AppPostInstallCommand="TempDir=$(mktemp -d);
cd $TempDir;
wget http://download.owncloud.org/community/owncloud-latest.tar.bz2;
aunpack owncloud-latest.tar.bz2;
mv owncloud $APACHE_DOCUMENTROOT/$AppWwwDir;
cd;
rmdir $TempDir"